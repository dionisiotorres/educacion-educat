<openerp>
    <data>
        <record id="view_sie_register_subject_tree" model="ir.ui.view">
            <field name="name">sie.register.subject.tree</field>
            <field name="model">sie.register.subject</field>
            <field name="arch" type="xml">
                <tree string="register of subject">
                    <field name="topic"/>
                    <field name="course_id"/>
                    <field name="student_id"/>
                    <field name="type"/>
                </tree>
            </field>
        </record>
        <record id="view_sie_register_subject_form" model="ir.ui.view">
            <field name="name">sie.register.subject.form</field>
            <field name="model">sie.register.subject</field>
            <field name="arch" type="xml">
                <form string="register of subject">
                    <sheet>
                        <group>
                            <field name="course_id"/>
                            <field name="student_id"/>
                            <field name="type"/>
                            <field name="topic"/>
                            <field name="enrollment_id" invisible="1"/>
                        </group>
                        <notebook>
                            <page string="Evaluator">
                                <group>
                                    <field name="faculty_ids" widget='many2many_tags'/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_sie_register_subject_search" model="ir.ui.view">
            <field name="name">sie.register.subject.search</field>
            <field name="model">sie.register.subject</field>
            <field name="arch" type="xml">
                <search string="register_subject">
                    <field name="name" select="True"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_sie_register_subject_view">
            <field name="name">register of subject</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sie.register.subject</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_sie_register_subject_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a new register_subject.
                </p>
            </field>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_sie_register_subject_view_form">
            <field name="act_window_id" ref="act_open_sie_register_subject_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_sie_register_subject_form"/>
        </record>
        <record model="ir.actions.act_window.view" id="act_open_sie_register_subject_view_tree">
            <field name="act_window_id" ref="act_open_sie_register_subject_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_sie_register_subject_tree"/>
        </record>
    </data>
</openerp>